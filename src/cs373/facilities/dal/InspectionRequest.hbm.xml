<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cs373.facilities.model.maintenance">
    <class name="InspectionRequest" table="inspectionrequest">
        <id name="id" type="int" access="field" column="id" />
        <property name="description" type="java.lang.String">
            <column name="description" />
        </property>
        <property name="technician" type="java.lang.String">
            <column name="technician" />
        </property>
    </class>
</hibernate-mapping>
